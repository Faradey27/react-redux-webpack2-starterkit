{
  "name": "react-redux-webpack2-starterkit",
  "version": "0.2.0",
  "description": "Boilerplate for react-redux project with webpack 2 and tests",
  "main": "src/index.js",
  "keywords": [
    "react",
    "webpack2",
    "redux",
    "tests",
    "nightwatch",
    "windows",
    "i18n"
  ],
  "author": {
    "name": "Andrii Tiertyshnyi",
    "email": "andrii.tiertyshnyi@gmail.com"
  },
  "contributors": [
    {
      "name": "Andrii Tiertyshnyi(Faradey27)",
      "url": "https://github.com/Faradey27"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Faradey27/react-redux-webpack2-starterkit"
  },
  "bugs": {
    "url": "https://github.com/Faradey27/react-redux-webpack2-starterkit/issues"
  },
  "scripts": {
    "start": "node server.js",
    "testserver": "npm run build && node testserver.js",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack -p && cp -R ./src/config ./dist",
    "test": "cross-env BABEL_ENV=test mocha --compilers js:babel-register --require ./test/testHelper.js --recursive ./src/**/*.spec.js",
    "test:unit": "cross-env BABEL_ENV=test mocha --compilers js:babel-register --require ./test/testHelper.js --recursive ./test/**/*.spec.js",
    "test:coverage": "cross-env BABEL_ENV=test nyc --require babel-core/register mocha --require ./test/testHelper.js --recursive ./test/**/*.spec.js",
    "test:e2e": "node nightwatch --test",
    "selenium:chrome": "java -jar -Dwebdriver.chrome.driver=\"node_modules/chromedriver/lib/chromedriver/chromedriver.exe\" node_modules/selenium-server/lib/runner/selenium-server-standalone-3.0.1.jar",
    "createLocaleFiles": "./bin/extract_lang.sh",
    "convertPOtoJson": "./bin/po2json.sh"
  },
  "dependencies": {
